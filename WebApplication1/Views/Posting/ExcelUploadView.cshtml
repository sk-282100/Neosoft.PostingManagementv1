@model PostingManagement.UI.Models.ExcelUploadViewModel@using Microsoft.AspNetCore.Http@{    ViewData["Title"] = "EmployeeMasterUpload";}@{    Layout = "~/Views/Shared/MainView.cshtml";}<script type="text/javascript">    $(document).ready(function () {        //var ID = HttpContext.Session.GetInt32("ExcelFileTypeCode").ToString();        $.ajax({            url: "@Url.Action("ShowUploadHistory", "Posting")",            type: "post",            data: {id: @ViewBag.ExcelFileTypeCode},            //data: { id: HttpContext.Session.GetInt32("ExcelFileTypeCode") },            success: function (data) {                console.log(data);                //// $("#test").load("/Home/GetAllAssignedRole");                $("#assign").replaceWith(data)            },            error: function () {                alert("Fails 1");            }        });    });</script><div class="pagetitle">    <h1>Excel Upload</h1>            <nav>                <ol class="breadcrumb">                    <li class="breadcrumb-item">Excel Upload</li>            <li class="breadcrumb-item active">@ViewBag.ExcelUploadFiletype</li>                </ol>            </nav></div><!-- End Page Title --><hr/><div class="row" >    <div class="col-md-4">        <form enctype="multipart/form-data" asp-action="ExcelUpload">            <div asp-validation-summary="ModelOnly" class="text-danger"></div>            <div class="form-group">                <input asp-for="FileType" value="@ViewBag.ExcelUploadFiletype" style="width:100%" class="form-control" hidden />                <span asp-validation-for="FileType" class="text-danger" id="ResumePathError"></span>            </div>            <div style="display:inline-flex;">                <div class="form-group">                    <input asp-for="ExcelFile" type="file" style="width:100%" class="form-control custome-file-input" />                    <span asp-validation-for="ExcelFile" class="text-danger" id="ResumePathError"></span>                </div>                <div class="form-group">                    <input type="submit" value="Upload" class="btn btn-success"                    style="margin-left:10px"/>                </div>            </div>        </form>    </div>    @if (ViewBag.ExcelUploadResponse != null)
    {        @if (ViewBag.ExcelUploadResponse.UploadStatus == "Success")
        {            <h4 class="txt-success p-3 d-flex align-items-center">@ViewBag.ExcelUploadResponse.SuccessCount</h4>        }
        else
        {            <h4 class="txt-danger p-3 d-flex align-items-center">Uploading Failed</h4>        };
    }</div><div id="assign">    <partial name="ShowUploadHistory"></div>