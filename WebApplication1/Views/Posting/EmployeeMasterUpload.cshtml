@model PostingManagement.UI.Models.ExcelUploadViewModel@{    ViewData["Title"] = "EmployeeMasterUpload";}@{    Layout = "~/Views/Shared/MainView.cshtml";}<h1>Excel Upload</h1><hr /><div class="row" >    <div class="col-md-4">        <form enctype="multipart/form-data" asp-action="ExcelUpload">            <div asp-validation-summary="ModelOnly" class="text-danger"></div>            <div class="form-group">                <input asp-for="FileType" value="@ViewBag.ExcelUploadFiletype" style="width:100%" class="form-control" hidden />                <span asp-validation-for="FileType" class="text-danger" id="ResumePathError"></span>            </div>            <div style="display:inline-flex;">                <div class="form-group">                    <input asp-for="ExcelFile" type="file" style="width:100%" class="form-control custome-file-input" />                    <span asp-validation-for="ExcelFile" class="text-danger" id="ResumePathError"></span>                </div>                <div class="form-group">                    <input type="submit" value="Upload" class="btn btn-success"                    style="margin-left:10px"/>                </div>            </div>        </form>    </div></div><script type="text/javascript">    $(function () {        $("#Upload").click(function () {            window.location.href = "@Url.Action("ShowEmployeeMaster", "Posting")";        });    });</script>