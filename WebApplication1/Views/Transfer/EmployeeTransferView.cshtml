<div class="col-xxl-12 col-xl-12">

    <div class="card info-card customers-card">

        <div class="card-body">
            <div class="d-flex flex-row justify-content-between">
                <h5 class="card-title">Generate Employee Transfer List<span></span></h5>
                <a class="btn btn-success" onclick="GenerateList()">Generate List</a>
            </div>
            <hr />
            <div class="row">
                
                <div class="col-md-12">
                    <table class="table responsive" id="employeeTransfer">
                        <thead>
                            <tr>
                                <th></th>
                                @*<th><input type="checkbox" name="select_all" id="example-select-all"></th>*@
                                <th></th>
                                <th>Employee Id</th>
                                <th>Name</th>  
                                <th>Scale Name</th>  
                                <th>Scale</th>  
                                <th>Designation</th>  
                                <th>Region</th>  
                                <th>Zone</th>  
                                <th>Movement Type</th>  
                                <th></th>
                                @*<th>De(Select)</th>*@
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/js/EmployeeTransfer.js"></script>

