@model IEnumerable<PostingManagement.UI.Models.EmployeeTransferModels.EmployeeTransferModel>

@{
    ViewData["Title"] = "GetEmployeesForTransfer";
    Layout = "~/Views/Shared/MainView.cshtml";
}

<link href="~/css/datatables-1.13.1.css" rel="stylesheet" />
<link href="~/css/datatables.buttons-2.3.2.css" rel="stylesheet" />
<link href="~/css/datatables-checkboxes-1.2.12.css" rel="stylesheet" />

<script src="~/lib/jquery/dist/jquery.datatables-1.13.1.js"></script>
<script src="~/lib/jquery/dist/datatable.buttons - 2.3.2.js"></script>
