@*@model PostingManagement.UI.Models.ExcelUploadViewModel*@<!DOCTYPE html><html lang="en"><head>    <meta charset="utf-8">    <meta content="width=device-width, initial-scale=1.0" name="viewport">    <meta name="viewport" content="width=device-width, initial-scale=1.0" />    <title>@ViewData["Title"] - UI.PostingManagement</title>    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />    <link rel="stylesheet" href="~/WebApplication1.styles.css" asp-append-version="true" />    <link rel="stylesheet" href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" />    <meta content="" name="description">    <meta content="" name="keywords">    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />    <!-- Vendor CSS Files -->    <link href="~/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">    <link href="~/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>    <!---Partial View Ajax Call->    <!-- Template Main CSS File -->    <link href="css/style.css" rel="stylesheet">    <script type="text/javascript" src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/notify/0.4.2/notify.min.js"></script>    <link href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.css" rel="stylesheet" type="text/css" />    <link href="https://cdn.datatables.net/1.13.1/css/dataTables.bootstrap.min.css" rel="stylesheet" type="text/css" /></head><body>    <!-- ======= Header ======= -->    <header id="header" class="header fixed-top d-flex align-items-center">        <div class="d-flex align-items-center justify-content-between">            <a asp-action="MainView" class="logo d-flex align-items-center">                <img src="~/img/logo.png" alt="" />                <span class="d-none d-lg-block">Posting Management</span>            </a>            <i class="bi bi-list toggle-sidebar-btn"></i>        </div><!-- End Logo -->        <nav class="header-nav ms-auto">            <ul class="d-flex align-items-center">                <li class="nav-item dropdown pe-3">                                       <a class="btn btn-danger " asp-action="Logout" asp-controller="Login">                                                <span> LogOut </span>                    </a>                </li>            </ul>        </nav>    </header><!-- End Header -->    <!-- ======= Sidebar ======= -->    <aside id="sidebar" class="sidebar">        <ul class="sidebar-nav" id="sidebar-nav">            <li class="nav-item">                <a class="nav-link " asp-action="ShowBranchMaster" asp-controller="Posting">                    <i class="bi bi-grid"></i>                    <span>Dashboard</span>                </a>            </li><!-- End Dashboard Nav -->            <li class="nav-item">                <a class="nav-link collapsed" data-bs-target="#excelupload-nav" data-bs-toggle="collapse">                    <i class="bi bi-menu-button-wide"></i><span>Excel Upload</span><i class="bi bi-chevron-down ms-auto"></i>                </a>                <ul id="excelupload-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="BranchMasterUpload" asp-controller="Posting">                            <i class="bi bi-person"></i>                            <span>Upload Branch Master</span>                        </a>                    </li><!-- End Branch Page Nav -->                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="EmployeeMasterUpload" asp-controller="Posting">                            <i class="bi bi-person"></i>                            <span>Employee Master</span>                        </a>                    </li><!-- End Inter Zonal Page Nav -->                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="InterRegionalPromotionUpload" asp-controller="Posting">                            <i class="bi bi-person"></i>                            <span>Upload Inter Regional Promotion</span>                        </a>                    </li><!-- End InterRegion Page Nav -->                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="InterRegionRequestTransferUpload" asp-controller="Posting">                            <i class="bi bi-person"></i>                            <span>Upload Inter Region Request Transfer</span>                        </a>                    </li><!-- End Promotion Preferrences Page Nav -->                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="InterZonalPromotionUpload" asp-controller="Posting">                            <i class="bi bi-person"></i>                            <span>Upload Inter Zonal Promotion</span>                        </a>                    </li><!-- End Promotion List Page Nav -->                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="InterZonalRequestTransferUpload" asp-controller="Posting">                            <i class="bi bi-person"></i>                            <span>Upload Inter Zonal Request Transfer</span>                        </a>                    </li><!-- End APAR Score Page Nav -->                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="RegionMasterUpload" asp-controller="Posting">                            <i class="bi bi-person"></i>                            <span>Upload Region Master</span>                        </a>                    </li><!-- End Promotion List Page Nav -->                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="ZoneMasterUpload" asp-controller="Posting">                            <i class="bi bi-card-list"></i>                            <span>Upload Zone Master</span>                        </a>                    </li><!-- End Register Page Nav -->                    <li class="nav-item">                        <a class="nav-link collapsed" asp-action="DepartmentMasterUpload" asp-controller="Posting">                            <i class="bi bi-box-arrow-in-right"></i>                            <span>Upload Department Master</span>                        </a>                    </li><!-- End Login Page Nav -->                </ul>        </ul>    </aside><!-- End Sidebar-->    <main id="main" class="main">        <div class="pagetitle">            <div class="container">                <main role="main" class="pb-3">                    @RenderBody()                </main>            </div>        </div><!-- End Page Title -->    </main><!-- End #main -->    <!-- ======= Footer ======= -->        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>    <script src="~/js/site.js" asp-append-version="true"></script>    @await RenderSectionAsync("Scripts", required: false)</body></html>